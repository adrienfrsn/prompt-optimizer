spring:
  application:
    name: backend
  datasource:
    url: your-database-url
    username: your-database-username
    password: your-database-password
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        dialect: org.hibernate.dialect.PostgreSQLDialect

openai:
  api:
    key: sk-XXXXXXXXXXXXXXXXXXXXXXXXXXXX # Remplacez par votre cl√© API OpenAI
    model: gpt-3.5-turbo # ou gpt-4
    maxTokens: 1000 # ou 2000 pour gpt-4

app:
  secret-key: your-secret
  expiration-time: 86400